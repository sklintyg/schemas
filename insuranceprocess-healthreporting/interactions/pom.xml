                  </wsdl>
                  <wsdlLocation>classpath:interactions/SendMedicalCertificateInteraction/SendMedicalCertificateInteraction_1.0_rivtabp20.wsdl</wsdlLocation>
                  <packagenames>
                    <packagename>urn:riv:insuranceprocess:healthreporting:SendMedicalCertificate:1:rivtabp20=se.inera.ifv.insuranceprocess.healthreporting.sendmedicalcertificate.v1.rivtabp20</packagename>
                    <packagename>urn:riv:insuranceprocess:healthreporting:SendMedicalCertificateResponder:1=se.inera.ifv.insuranceprocess.healthreporting.sendmedicalcertificateresponder.v1</packagename>
                  </packagenames>
                </wsdlOption>
                
                <wsdlOption>
                  <wsdl>
                    ${wsdl.directory}/SendMedicalCertificateQuestionInteraction/SendMedicalCertificateQuestionInteraction_1.0_rivtabp20.wsdl
                  </wsdl>
                  <wsdlLocation>classpath:interactions/SendMedicalCertificateQuestionInteraction/SendMedicalCertificateQuestionInteraction_1.0_rivtabp20.wsdl</wsdlLocation>
                  <packagenames>
                    <packagename>urn:riv:insuranceprocess:healthreporting:SendMedicalCertificateQuestion:1:rivtabp20=se.inera.ifv.insuranceprocess.healthreporting.sendmedicalcertificatequestion.v1.rivtabp20</packagename>
                    <packagename>urn:riv:insuranceprocess:healthreporting:SendMedicalCertificateQuestionResponder:1=se.inera.ifv.insuranceprocess.healthreporting.sendmedicalcertificatequestionresponder.v1</packagename>
                  </packagenames>
                </wsdlOption>
                
                <wsdlOption>
                  <wsdl>
                    ${wsdl.directory}/SetCertificateStatusInteraction/SetCertificateStatusInteraction_1.0_rivtabp20.wsdl
                  </wsdl>
                  <wsdlLocation>classpath:interactions/SetCertificateStatusInteraction/SetCertificateStatusInteraction_1.0_rivtabp20.wsdl</wsdlLocation>
                  <packagenames>
                    <packagename>urn:riv:insuranceprocess:healthreporting:SetCertificateStatus:1:rivtabp20=se.inera.ifv.insuranceprocess.healthreporting.setcertificatestatus.v1.rivtabp20</packagename>
                    <packagename>urn:riv:insuranceprocess:healthreporting:SetCertificateStatusResponder:1=se.inera.ifv.insuranceprocess.healthreporting.setcertificatestatusresponder.v1</packagename>
                  </packagenames>
                </wsdlOption>
                
                <wsdlOption>
                  <wsdl>
                    ${wsdl.directory}/SetConsentInteraction/SetConsentInteraction_1.0_rivtabp20.wsdl
                  </wsdl>
                  <wsdlLocation>classpath:interactions/SetConsentInteraction/SetConsentInteraction_1.0_rivtabp20.wsdl</wsdlLocation>
                  <packagenames>
                    <packagename>urn:riv:insuranceprocess:healthreporting:SetConsent:1:rivtabp20=se.inera.ifv.insuranceprocess.healthreporting.setconsent.v1.rivtabp20</packagename>
                    <packagename>urn:riv:insuranceprocess:healthreporting:SetConsentResponder:1=se.inera.ifv.insuranceprocess.healthreporting.setconsentresponder.v1</packagename>
                  </packagenames>
                </wsdlOption>
                
                <wsdlOption>
                  <wsdl>
                    ${wsdl.directory}/ReceiveMedicalCertificateAnswerInteraction/ReceiveMedicalCertificateAnswerInteraction_1.0_rivtabp20.wsdl
                  </wsdl>
                  <wsdlLocation>classpath:interactions/ReceiveMedicalCertificateAnswerInteraction/ReceiveMedicalCertificateAnswerInteraction_1.0_rivtabp20.wsdl</wsdlLocation>
                  <packagenames>
                    <packagename>urn:riv:insuranceprocess:healthreporting:ReceiveMedicalCertificateAnswer:1:rivtabp20=se.inera.webcert.receivemedicalcertificateanswer.v1.rivtabp20</packagename>
                    <packagename>urn:riv:insuranceprocess:healthreporting:ReceiveMedicalCertificateAnswerResponder:1=se.inera.webcert.receivemedicalcertificateanswerresponder.v1</packagename>
                  </packagenames>
                </wsdlOption>
                
                <wsdlOption>
                  <wsdl>
                    ${wsdl.directory}/ReceiveMedicalCertificateQuestionInteraction/ReceiveMedicalCertificateQuestionInteraction_1.0_rivtabp20.wsdl
                  </wsdl>
                  <wsdlLocation>classpath:interactions/ReceiveMedicalCertificateQuestionInteraction/ReceiveMedicalCertificateQuestionInteraction_1.0_rivtabp20.wsdl</wsdlLocation>
                  <packagenames>
                    <packagename>urn:riv:insuranceprocess:healthreporting:ReceiveMedicalCertificateQuestion:1:rivtabp20=se.inera.webcert.receivemedicalcertificatequestion.v1.rivtabp20</packagename>
                    <packagename>urn:riv:insuranceprocess:healthreporting:ReceiveMedicalCertificateQuestionResponder:1=se.inera.webcert.receivemedicalcertificatequestionsponder.v1</packagename>
                  </packagenames>
                </wsdlOption>
                                
              </wsdlOptions>
            </configuration>
          </execution>
        </executions>
        <dependencies>
          <dependency>
            <groupId>org.jvnet.jaxb2_commons</groupId>
            <artifactId>jaxb2-basics</artifactId>
            <version>${jaxb.commons.version}</version>
          </dependency>
        </dependencies>
      </plugin>
    </plugins>
  </build>
  
</project>