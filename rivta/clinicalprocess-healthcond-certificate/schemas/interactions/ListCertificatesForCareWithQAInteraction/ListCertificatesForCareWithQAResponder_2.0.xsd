			<xs:any namespace="##other" processContents="lax" minOccurs="0"
				maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

<xs:complexType name="ListItem">
		<xs:sequence>
			<xs:element name="intyg" type="core:IntygListInfoMedLedtext"/>
			<xs:element name="handelser" type="tns:HandelseList"/>
			<xs:element name="fragorOchSvar" type="core:FragorOchSvar"/>
			<xs:any namespace="##other" processContents="lax" minOccurs="0"
				maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="HandelseList">
		<xs:sequence>
			<xs:element name="handelse" type="core:Handelse" maxOccurs="unbounded" />
			<xs:any namespace="##other" processContents="lax" minOccurs="0"
				maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>
	
</xs:schema>
