<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:core="urn:riv:clinicalprocess:healthcond:certificate:2"
	xmlns:types="urn:riv:clinicalprocess:healthcond:certificate:types:2"	
	xmlns:tns="urn:riv:clinicalprocess:healthcond:certificate:ListCertificatesForCareWithQAResponder:2"
	targetNamespace="urn:riv:clinicalprocess:healthcond:certificate:ListCertificatesForCareWithQAResponder:2"
	elementFormDefault="qualified" attributeFormDefault="unqualified"
	version="2.0">

	<xs:import namespace="urn:riv:clinicalprocess:healthcond:certificate:2"
		schemaLocation="../../core_components/clinicalprocess_healthcond_certificate_2.0.xsd" />
	<xs:import namespace="urn:riv:clinicalprocess:healthcond:certificate:types:2"
