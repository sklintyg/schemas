<wsdl:definitions name='RegisterCertificateInteraction' xmlns:wsdl='http://schemas.xmlsoap.org/wsdl/'
                  xmlns:soap='http://schemas.xmlsoap.org/wsdl/soap/' xmlns:xs='http://www.w3.org/2001/XMLSchema'
                  xmlns:tjsr='urn:riv:clinicalprocess:healthcond:certificate:RegisterCertificateResponder:1'
                  xmlns:tns='urn:riv:clinicalprocess:healthcond:certificate:RegisterCertificate:1:rivtabp21'
                  targetNamespace='urn:riv:clinicalprocess:healthcond:certificate:RegisterCertificate:1:rivtabp21'
                  xmlns:itr='urn:riv:itintegration:registry:1'>
  <xs:annotation>
    <xs:documentation>
      Tj채nsteinteraktionens namn: RegisterCertificateInteraction
      Beskrivning:
      Interaction to register a medical certificate.
      Revisioner:
      2013-11-20: Utkast Version 1.0, mats.ekhammar@callistaenterprise.se
      Tj채nstedom채n: clinicalprocess:healthcond:certificate
      Tj채nsteinteraktionstyp: Intyg
      WS-profil: RIVTABP21
    </xs:documentation>
  </xs:annotation>
  <wsdl:types>
    <xs:schema targetNamespace='urn:riv:clinicalprocess:healthcond:certificate:RegisterCertificate:1:rivtabp21'>
      <xs:import schemaLocation='RegisterCertificateResponder_1.0.xsd'
                 namespace='urn:riv:clinicalprocess:healthcond:certificate:RegisterCertificateResponder:1'/>
      <xs:import schemaLocation='../../core_components/itintegration_registry_1.0.xsd'
                 namespace='urn:riv:itintegration:registry:1'/>
    </xs:schema>
  </wsdl:types>
  <wsdl:message name='RegisterCertificateRequest'>
    <wsdl:part name='LogicalAddress' element='itr:LogicalAddress'>
    </wsdl:part>
    <wsdl:part name='parameters' element='tjsr:RegisterCertificate'/>
  </wsdl:message>
  <wsdl:message name='RegisterCertificateResponse'>
    <wsdl:part name='parameters' element='tjsr:RegisterCertificateResponse'/>
  </wsdl:message>
  <wsdl:portType name='RegisterCertificateResponderInterface'>
    <wsdl:operation name='RegisterCertificate'>
      <wsdl:input message='tns:RegisterCertificateRequest'/>
      <wsdl:output message='tns:RegisterCertificateResponse'/>
    </wsdl:operation>
  </wsdl:portType>
  <wsdl:binding name='RegisterCertificateResponderBinding' type='tns:RegisterCertificateResponderInterface'>
    <soap:binding style='document' transport='http://schemas.xmlsoap.org/soap/http'/>
    <wsdl:operation name='RegisterCertificate'>
      <soap:operation
              soapAction='urn:riv:clinicalprocess:healthcond:certificate:RegisterCertificateResponder:1:RegisterCertificate'
              style='document'/>
      <wsdl:input>
        <soap:header use='literal' message='tns:RegisterCertificateRequest' part='LogicalAddress'/>
        <soap:body use='literal' parts='parameters'/>
      </wsdl:input>
      <wsdl:output>
        <soap:body use='literal'/>
      </wsdl:output>
    </wsdl:operation>
  </wsdl:binding>
  <wsdl:service name='RegisterCertificateResponderService'>
    <wsdl:port name='RegisterCertificateResponderPort' binding='tns:RegisterCertificateResponderBinding'>
      <soap:address location='http://tempuri.org'/>
    </wsdl:port>
  </wsdl:service>
</wsdl:definitions>
