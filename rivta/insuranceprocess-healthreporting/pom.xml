<?xml version="1.0"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">

  <modelVersion>4.0.0</modelVersion>

  <parent>
    <groupId>se.inera.intyg.common</groupId>
    <artifactId>common-pom</artifactId>
    <version>1.0.6</version>
  </parent>

  <groupId>se.inera.intyg.schemas.insuranceprocess.healthreporting.v1</groupId>
  <artifactId>insuranceprocess-healthreporting</artifactId>
  <version>0-SNAPSHOT</version>
  <packaging>jar</packaging>

  <name>Insurance Process Health Reporting v1</name>

  <properties>
    <schemas-common.version>0-SNAPSHOT</schemas-common.version>
    <schemas-common.version.range>(,2.2.0]</schemas-common.version.range>

    <schemas.directory>${basedir}/schemas/core_components</schemas.directory>
    <wsdl.directory>${basedir}/schemas/interactions</wsdl.directory>
  </properties>

  <dependencyManagement>
    <dependencies>
      <dependency>
        <groupId>se.inera.intyg.schemas</groupId>
        <artifactId>schemas-common</artifactId>
        <version>${schemas-common.version}</version>
      </dependency>
    </dependencies>
  </dependencyManagement>

  <dependencies>
    <dependency>
      <groupId>se.inera.intyg.schemas</groupId>
      <artifactId>schemas-common</artifactId>
    </dependency>
    <dependency>
      <groupId>org.apache.cxf</groupId>
      <artifactId>cxf-rt-frontend-jaxws</artifactId>
    </dependency>
    <dependency>
      <groupId>org.apache.cxf</groupId>
      <artifactId>cxf-rt-transports-http</artifactId>
    </dependency>
    <dependency>
      <groupId>joda-time</groupId>
      <artifactId>joda-time</artifactId>
    </dependency>
    <dependency>
      <groupId>org.jvnet.jaxb2_commons</groupId>
      <artifactId>jaxb2-basics-runtime</artifactId>
    </dependency>
  </dependencies>

  <build>
    <plugins>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>versions-maven-plugin</artifactId>
        <configuration>
          <includeProperties>
            schemas-common.version
          </includeProperties>
          <properties>
            <property>
              <name>schemas-common.version</name>
              <version>${schemas-common.version.range}</version>
            </property>
          </properties>
        </configuration>
      </plugin>

      <plugin>
        <groupId>org.jvnet.jaxb2.maven2</groupId>
        <artifactId>maven-jaxb2-plugin</artifactId>
        <version>0.11.0</version>
        <executions>
          <execution>
            <phase>generate-sources</phase>
            <goals>
              <goal>generate</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <args>
            <!-- Enable JAXB extensions -->
            <arg>-XhashCode</arg>
            <arg>-Xequals</arg>
            <arg>-XtoString</arg>
            <arg>-Xinheritance</arg>
          </args>
          <extension>true</extension>
          <forceRegenerate>true</forceRegenerate>
          <bindingDirectory>${basedir}/insuranceprocess-healthreporting/bindings</bindingDirectory>
          <bindingIncludes>
            <bindingInclude>classpath:/bindings/general-binding.xjb</bindingInclude>
            <bindingInclude>package-customization.xjb</bindingInclude>
          </bindingIncludes>
          <catalog>${basedir}/insuranceprocess-healthreporting/catalog/catalog.xml</catalog>
          <schemas>
            <schema>
              <fileset>
                <directory>${schemas.directory}</directory>
                <includes>
                  <include>iso_dt_subset_1.0.xsd</include>
                  <include>insuranceprocess_certificate_1.0.xsd</include>
                  <include>insuranceprocess_healthreporting_2.0.xsd</include>
                  <include>MedicalCertificateQuestionsAnswers_1.0.xsd</include>
                  <include>MU7263-RIV_3.1.xsd</include>
                  <include>ws-addressing-1.0.xsd</include>
                </includes>
              </fileset>
            </schema>
          </schemas>
          <debug>false</debug>
          <verbose>false</verbose>
          <plugins>
            <dependency>
              <groupId>org.jvnet.jaxb2_commons</groupId>
              <artifactId>jaxb2-basics</artifactId>
              <version>${jaxb.commons.version}</version>
            </dependency>
          </plugins>
        </configuration>
      </plugin>

      <plugin>
        <groupId>org.apache.cxf</groupId>
        <artifactId>cxf-codegen-plugin</artifactId>
        <executions>
          <execution>
            <id>wsdl2java-external-interfaces</id>
            <goals>
              <goal>wsdl2java</goal>
            </goals>
            <phase>generate-sources</phase>
            <configuration>
              <defaultOptions>
                <extraargs>
                  <!-- Exclude generation of code from core components -->
                  <extraarg>-nexclude</extraarg>
                  <extraarg>urn:iso:21090:dt:1=iso.v21090.dt.v1</extraarg>
                  <extraarg>-nexclude</extraarg>
                  <extraarg>http://www.w3.org/2005/08/addressing=org.w3.wsaddressing10</extraarg>
                  <extraarg>-nexclude</extraarg>
                  <extraarg>urn:riv:insuranceprocess:certificate:1=se.inera.ifv.insuranceprocess.certificate.v1</extraarg>
                  <extraarg>-nexclude</extraarg>
                  <extraarg>urn:riv:insuranceprocess:healthreporting:2=se.inera.ifv.insuranceprocess.healthreporting.v2</extraarg>
                  <extraarg>-nexclude</extraarg>
                  <extraarg>urn:riv:insuranceprocess:healthreporting:medcertqa:1=se.inera.webcert.medcertqa.v1</extraarg>
                  <extraarg>-nexclude</extraarg>
                  <extraarg>urn:riv:insuranceprocess:healthreporting:mu7263:3=se.inera.ifv.insuranceprocess.healthreporting.mu7263.v3</extraarg>
                </extraargs>
              </defaultOptions>

              <wsdlOptions>
                <wsdlOption>
                  <wsdl>
                    ${wsdl.directory}/GetCertificateContent/GetCertificateContentInteraction.wsdl
                  </wsdl>
                  <packagenames>
                    <packagename>
                      urn:riv:insuranceprocess:healthreporting:GetCertificateContent=se.inera.ifv.insuranceprocess.healthreporting.getcertificatecontentresponder.v1
                    </packagename>
                    <packagename>
                      urn:riv:insuranceprocess:healthreporting:GetCertificateContentResponder:1=se.inera.ifv.insuranceprocess.healthreporting.getcertificatecontentresponder.v1
                    </packagename>
                  </packagenames>
                </wsdlOption>

                <wsdlOption>
                  <wsdl>
                    ${wsdl.directory}/GetCertificateInteraction/GetCertificateInteraction_1.0_rivtabp20.wsdl
                  </wsdl>
                  <packagenames>
                    <packagename>
                      urn:riv:insuranceprocess:healthreporting:GetCertificate:1:rivtabp20=se.inera.ifv.insuranceprocess.healthreporting.getcertificate.v1.rivtabp20
                    </packagename>
                    <packagename>
                      urn:riv:insuranceprocess:healthreporting:GetCertificateResponder:1=se.inera.ifv.insuranceprocess.healthreporting.getcertificateresponder.v1
                    </packagename>
                  </packagenames>
                </wsdlOption>

                <wsdlOption>
                  <wsdl>
                    ${wsdl.directory}/GetConsentInteraction/GetConsentInteraction_1.0_rivtabp20.wsdl
                  </wsdl>
                  <packagenames>
                    <packagename>
                      urn:riv:insuranceprocess:healthreporting:GetConsent:1:rivtabp20=se.inera.ifv.insuranceprocess.healthreporting.getconsent.v1.rivtabp20
                    </packagename>
                    <packagename>
                      urn:riv:insuranceprocess:healthreporting:GetConsentResponder:1=se.inera.ifv.insuranceprocess.healthreporting.getconsentresponder.v1
                    </packagename>
                  </packagenames>
                </wsdlOption>

                <wsdlOption>
                  <wsdl>
                    ${wsdl.directory}/ListCertificatesInteraction/ListCertificatesInteraction_1.0_rivtabp20.wsdl
                  </wsdl>
                  <packagenames>
                    <packagename>
                      urn:riv:insuranceprocess:healthreporting:ListCertificates:1:rivtabp20=se.inera.ifv.insuranceprocess.healthreporting.listcertificates.v1.rivtabp20
                    </packagename>
                    <packagename>
                      urn:riv:insuranceprocess:healthreporting:ListCertificatesResponder:1=se.inera.ifv.insuranceprocess.healthreporting.listcertificatesresponder.v1
                    </packagename>
                  </packagenames>
                </wsdlOption>

                <wsdlOption>
                  <wsdl>
                    ${wsdl.directory}/RegisterMedicalCertificateInteraction/RegisterMedicalCertificateInteraction_3.1_rivtabp20.wsdl
                  </wsdl>
                  <packagenames>
                    <packagename>
                      urn:riv:insuranceprocess:healthreporting:RegisterMedicalCertificate:3:rivtabp20=se.inera.ifv.insuranceprocess.healthreporting.registermedicalcertificate.v3.rivtabp20
                    </packagename>
                    <packagename>
                      urn:riv:insuranceprocess:healthreporting:RegisterMedicalCertificateResponder:3=se.inera.ifv.insuranceprocess.healthreporting.registermedicalcertificateresponder.v3
                    </packagename>
                  </packagenames>
                </wsdlOption>

                <wsdlOption>
                  <wsdl>
                    ${wsdl.directory}/RevokeMedicalCertificateInteraction/RevokeMedicalCertificateInteraction_1.0_rivtabp20.wsdl
                  </wsdl>
                  <packagenames>
                    <packagename>
                      urn:riv:insuranceprocess:healthreporting:RevokeMedicalCertificate:1:rivtabp20=se.inera.ifv.insuranceprocess.healthreporting.revokemedicalcertificate.v1.rivtabp20
                    </packagename>
                    <packagename>
                      urn:riv:insuranceprocess:healthreporting:RevokeMedicalCertificateResponder:1=se.inera.ifv.insuranceprocess.healthreporting.revokemedicalcertificateresponder.v1
                    </packagename>
                  </packagenames>
                </wsdlOption>

                <wsdlOption>
                  <wsdl>
                    ${wsdl.directory}/SendMedicalCertificateAnswerInteraction/SendMedicalCertificateAnswerInteraction_1.0_rivtabp20.wsdl
                  </wsdl>
                  <packagenames>
                    <packagename>
                      urn:riv:insuranceprocess:healthreporting:SendMedicalCertificateAnswer:1:rivtabp20=se.inera.ifv.insuranceprocess.healthreporting.sendmedicalcertificateanswer.v1.rivtabp20
                    </packagename>
                    <packagename>
                      urn:riv:insuranceprocess:healthreporting:SendMedicalCertificateAnswerResponder:1=se.inera.ifv.insuranceprocess.healthreporting.sendmedicalcertificateanswerresponder.v1
                    </packagename>
                  </packagenames>
                </wsdlOption>

                <wsdlOption>
                  <wsdl>
                    ${wsdl.directory}/SendMedicalCertificateInteraction/SendMedicalCertificateInteraction_1.0_rivtabp20.wsdl
                  </wsdl>
                  <packagenames>
                    <packagename>
                      urn:riv:insuranceprocess:healthreporting:SendMedicalCertificate:1:rivtabp20=se.inera.ifv.insuranceprocess.healthreporting.sendmedicalcertificate.v1.rivtabp20
                    </packagename>
                    <packagename>
                      urn:riv:insuranceprocess:healthreporting:SendMedicalCertificateResponder:1=se.inera.ifv.insuranceprocess.healthreporting.sendmedicalcertificateresponder.v1
                    </packagename>
                  </packagenames>
                </wsdlOption>

                <wsdlOption>
                  <wsdl>
                    ${wsdl.directory}/SendMedicalCertificateQuestionInteraction/SendMedicalCertificateQuestionInteraction_1.0_rivtabp20.wsdl
                  </wsdl>
                  <packagenames>
                    <packagename>
                      urn:riv:insuranceprocess:healthreporting:SendMedicalCertificateQuestion:1:rivtabp20=se.inera.ifv.insuranceprocess.healthreporting.sendmedicalcertificatequestion.v1.rivtabp20
                    </packagename>
                    <packagename>
                      urn:riv:insuranceprocess:healthreporting:SendMedicalCertificateQuestionResponder:1=se.inera.ifv.insuranceprocess.healthreporting.sendmedicalcertificatequestionresponder.v1
                    </packagename>
                  </packagenames>
                </wsdlOption>

                <wsdlOption>
                  <wsdl>
                    ${wsdl.directory}/SetCertificateStatusInteraction/SetCertificateStatusInteraction_1.0_rivtabp20.wsdl
                  </wsdl>
                  <packagenames>
                    <packagename>
                      urn:riv:insuranceprocess:healthreporting:SetCertificateStatus:1:rivtabp20=se.inera.ifv.insuranceprocess.healthreporting.setcertificatestatus.v1.rivtabp20
                    </packagename>
                    <packagename>
                      urn:riv:insuranceprocess:healthreporting:SetCertificateStatusResponder:1=se.inera.ifv.insuranceprocess.healthreporting.setcertificatestatusresponder.v1
                    </packagename>
                  </packagenames>
                </wsdlOption>

                <wsdlOption>
                  <wsdl>
                    ${wsdl.directory}/SetCertificateArchivedInteraction/SetCertificateArchivedInteraction_1.0_rivtabp20.wsdl
                  </wsdl>
                  <packagenames>
                    <packagename>
                      urn:riv:insuranceprocess:healthreporting:SetCertificateArchived:1:rivtabp20=se.inera.ifv.insuranceprocess.healthreporting.setcertificatearchived.v1.rivtabp20
                    </packagename>
                    <packagename>
                      urn:riv:insuranceprocess:healthreporting:SetCertificateArchivedResponder:1=se.inera.ifv.insuranceprocess.healthreporting.setcertificatearchivedresponder.v1
                    </packagename>
                  </packagenames>
                </wsdlOption>

                <wsdlOption>
                  <wsdl>
                    ${wsdl.directory}/SetConsentInteraction/SetConsentInteraction_1.0_rivtabp20.wsdl
                  </wsdl>
                  <packagenames>
                    <packagename>
                      urn:riv:insuranceprocess:healthreporting:SetConsent:1:rivtabp20=se.inera.ifv.insuranceprocess.healthreporting.setconsent.v1.rivtabp20
                    </packagename>
                    <packagename>
                      urn:riv:insuranceprocess:healthreporting:SetConsentResponder:1=se.inera.ifv.insuranceprocess.healthreporting.setconsentresponder.v1
                    </packagename>
                  </packagenames>
                </wsdlOption>

                <wsdlOption>
                  <wsdl>
                    ${wsdl.directory}/ReceiveMedicalCertificateAnswerInteraction/ReceiveMedicalCertificateAnswerInteraction_1.0_rivtabp20.wsdl
                  </wsdl>
                  <packagenames>
                    <packagename>
                      urn:riv:insuranceprocess:healthreporting:ReceiveMedicalCertificateAnswer:1:rivtabp20=se.inera.webcert.receivemedicalcertificateanswer.v1.rivtabp20
                    </packagename>
                    <packagename>
                      urn:riv:insuranceprocess:healthreporting:ReceiveMedicalCertificateAnswerResponder:1=se.inera.webcert.receivemedicalcertificateanswerresponder.v1
                    </packagename>
                  </packagenames>
                </wsdlOption>

                <wsdlOption>
                  <wsdl>
                    ${wsdl.directory}/ReceiveMedicalCertificateQuestionInteraction/ReceiveMedicalCertificateQuestionInteraction_1.0_rivtabp20.wsdl
                  </wsdl>
                  <packagenames>
                    <packagename>
                      urn:riv:insuranceprocess:healthreporting:ReceiveMedicalCertificateQuestion:1:rivtabp20=se.inera.webcert.receivemedicalcertificatequestion.v1.rivtabp20
                    </packagename>
                    <packagename>
                      urn:riv:insuranceprocess:healthreporting:ReceiveMedicalCertificateQuestionResponder:1=se.inera.webcert.receivemedicalcertificatequestionsponder.v1
                    </packagename>
                  </packagenames>
                </wsdlOption>

              </wsdlOptions>
            </configuration>
          </execution>
        </executions>
      </plugin>

      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>build-helper-maven-plugin</artifactId>
        <version>${buildnumber-maven-plugin.version}</version>
        <executions>
          <execution>
            <id>add-source</id>
            <phase>generate-sources</phase>
            <goals>
              <goal>add-source</goal>
            </goals>
            <configuration>
              <sources>
                <source>target/generated-sources/xjc</source>
                <source>target/generated-sources/cxf</source>
              </sources>
            </configuration>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>

  <repositories>
    <repository>
      <id>callista-release</id>
      <url>http://repository-callistasoftware.forge.cloudbees.com/release/</url>
      <releases>
        <enabled>true</enabled>
      </releases>
      <snapshots>
        <enabled>true</enabled>
      </snapshots>
    </repository>
    <repository>
      <id>callista-snapshot</id>
      <url>http://repository-callistasoftware.forge.cloudbees.com/snapshot/</url>
      <releases>
        <enabled>true</enabled>
      </releases>
      <snapshots>
        <enabled>true</enabled>
      </snapshots>
    </repository>
    <repository>
      <id>thirdparty</id>
      <url>http://repository-callistasoftware.forge.cloudbees.com/3rdparty</url>
      <releases>
        <enabled>true</enabled>
      </releases>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
    </repository>
  </repositories>

</project>
